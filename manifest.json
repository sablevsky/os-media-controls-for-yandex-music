{
  "name": "os-media-controls-for-yandex-music",
  "description": "Add OS media controls for yandex music web app.",
  "icons": {
    "64": "icon_64.png",
    "128": "icon_128.png",
    "256": "icon_256.png",
    "512": "icon_512.png"
  },
  "version": "1.1.6",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://music.yandex.by/*", "https://music.yandex.ru/*"],
      "run_at": "document_end",
      "js": ["content.js"]
    }
  ]
}
